{
  "name": "@env42/core",
  "version": "0.0.2",
  "description": "",
  "exports": {
    ".": "./dist/index.js",
    "./scripts": "./dist/scripts/index.js"
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "compile": "rm -rf dist && tsc -p tsconfig.prod.json",
    "tscpaths": "tscpaths -p ./tsconfig.json -s . -o ./dist",
    "build": "pnpm compile && pnpm tscpaths",
    "do-pack": "pnpm pack --pack-destination .build",
    "do-publish": "npm publish ./.build/nano-di-vitest-0.1.0.tgz --access public",
    "deploy": "npm run build && npm run do-pack && npm run do-publish",
    "test": "vitest run",
    "test:ci": "pnpm run test --coverage --silent",
    "test:watch": "vitest watch",
    "rebuild": "rm -rf dist && pnpm build",
    "lint": "eslint .",
    "typecheck": "pnpm compile --noEmit",
    "lint:fix": "pnpm lint --fix",
    "lint:ci": "pnpm lint --max-warnings 0",
    "ci:checks": "pnpm run typecheck && pnpm run lint:ci && pnpm run test:ci",
    "dev:done": "pnpm ci:checks && pnpm build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.7",
    "lodash.set": "^4.3.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.7",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitest/coverage-v8": "^0.32.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.8",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.0"
  }
}
